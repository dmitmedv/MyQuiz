{
  "name": "myquiz",
  "version": "1.0.0",
  "description": "A Quizlet-like application for learning foreign words - Production Ready",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec \"ts-node --compiler-options '{\\\"module\\\":\\\"commonjs\\\"}'\" src/server/index.ts",
    "dev:client": "cd client && npm start",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "cd client && npm run build",
    "start": "node dist/server/index.js",
    "install:client": "cd client && npm install"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.17.1",
    "concurrently": "^8.2.2",
    "jest": "^30.1.1",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "quiz",
    "learning",
    "vocabulary",
    "foreign-words"
  ],
  "author": "",
  "license": "MIT"
}
